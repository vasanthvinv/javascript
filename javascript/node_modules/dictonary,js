class MyClass {
  constructor(word, mean) {
    this.property1 = word;
    this.property2 = mean;
    const dictionary = {};
  }

   addWordToObject() {
    const word = prompt("Enter the word:");
    const meaning = prompt("Enter the meaning:");
    if (word && meaning) {
        dictionary[word] = meaning;
        displayDictionary();
    } else {
        alert("Both word and meaning are required!");
    }
}
}
function displayDictionary() {
    console.log(dictionary);
  }
  const myObject = new MyClass();
  myObject.addWordToObject();
